<p id="notice"><%= notice %></p>

<h1>Listing Jam Sessions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jam_sessions.each do |jam_session| %>
      <tr>
        <td><%= link_to 'Show', jam_session %></td>
        <td><%= link_to 'Edit', edit_jam_session_path(jam_session) %></td>
        <td><%= link_to 'Destroy', jam_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jam session', new_jam_session_path %>
